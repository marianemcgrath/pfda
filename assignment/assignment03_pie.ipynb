   "source": [
    "# We need to make sure we'll go though the email domains only, not individually each email.\n",
    "# So we want to read ONLY what is after the '@' symbol under 'Email'.\n",
    "\n",
    "df['domain']= df['Email'].str.split('@').str[1]\n",
    "\n",
    "# Source: "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "84f3a3ee",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "domain\n",
      "example.org    34.1\n",
      "example.com    33.9\n",
      "example.net    32.0\n",
      "Name: count, dtype: float64\n"
     ]
    }
   ],
   "source": [
    "# Now, we need to know how many of each domain there is in the file\n",
    "domain_counts = df[\"domain\"].value_counts()\n",
    "\n",
    "# And transform it to percentage.\n",
    "domain_percentages = (domain_counts / len(df)) * 100\n",
    "\n",
    "# Let's see what numbers we'll be looking at\n",
    "print(domain_percentages.round(2))\n",
    "\n",
    "# Source: https://stackoverflow.com/questions/56294597/how-to-create-a-pie-chart-from-csv-file-using-python\n",
    "# Source: https://www.analyticsvidhya.com/blog/2024/02/pie-chart-matplotlib/"
   ]
  },
